{  
    "Logging": {  
      "PathFormat": "Logs/Core.Service.Skraper3.{Date}.log",  
      "LogLevel": {  
        "Default": "Debug",  
        "System": "Information",  
        "Microsoft": "Information"  
      }  
    },  
    "FrequencyInMilliseconds": "15000",
    "SubscriptionsFileAndPath": "Subscriptions.json",
    "Serilog": {  
      "MinimumLevel": "Debug",  
      "WriteTo": [  
        {  
          "Name": "RollingFile",  
          "Args": {  
            "logDirectory": ".\\Logs",  
            "fileSizeLimitBytes": 1024,  
            "pathFormat": "Logs/Core.Service.Skraper3.{Date}.log",  
            "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"  
          }  
        }  
      ]  
    }  
  }  
    